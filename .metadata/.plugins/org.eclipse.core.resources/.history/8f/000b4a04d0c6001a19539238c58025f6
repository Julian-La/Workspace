package collections;

public class Type_Program {
	public static void main(String[] args) {
		DiaryEntry e = new DiaryEntry("Booktitle", "Interesting thoughts on humanity.");
		HideMessage h = new HideMessage("Hello World");
		HideMessage n = new HideMessage(e);
		
		System.out.println(h.getMessage("test"));
		System.out.println(n.getMessage("test"));
	}

}
