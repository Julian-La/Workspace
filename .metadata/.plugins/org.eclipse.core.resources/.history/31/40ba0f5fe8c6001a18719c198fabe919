package collections;

public class Type_Program {
	public static void main(String[] args) {

		HideMessage<String> h = new HideMessage<String>("Hello World");

		System.out.println(h.getMessage("test"));
//		Way of getting an object in:
		HideMessage<DiaryEntry> q = new HideMessage<DiaryEntry>(new DiaryEntry("Booktitle","This is the first page of many..."));		
		Object result = h.getMessage("test");
		if (result instanceof DiaryEntry) {
			DiaryEntry e = (DiaryEntry) result;
		}
		System.out.println("");
	}

}
